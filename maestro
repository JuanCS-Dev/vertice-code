#!/usr/bin/env python3
"""
Maestro Shell v7.0 Entry Point
"""
import sys
from pathlib import Path

# Add project to path
project_root = Path(__file__).parent
sys.path.insert(0, str(project_root))

# Load .env if present
from dotenv import load_dotenv
load_dotenv()

# Launch
from qwen_dev_cli.maestro import app

if __name__ == "__main__":
    app()
