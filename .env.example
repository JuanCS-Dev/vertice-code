# HuggingFace Inference API Token
# Get your token at: https://huggingface.co/settings/tokens
HF_TOKEN=your_huggingface_token_here

# SambaNova Cloud API Key (ultra-fast inference)
# Get your key at: https://cloud.sambanova.ai/
SAMBANOVA_API_KEY=your_sambanova_key_here

# Blaxel API Key (Agentic Network platform)
# Platform: https://app.blaxel.ai/
BLAXEL_API_KEY=your_blaxel_key_here

# Optional: Enable Ollama for local inference
OLLAMA_ENABLED=false

# Optional: Gradio port (default: 7860)
GRADIO_PORT=7860
