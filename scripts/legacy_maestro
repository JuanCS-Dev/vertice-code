#!/usr/bin/env python3
"""
MAESTRO v10.0 INTEGRATED - Entry Point
"""
import sys
from pathlib import Path

# Add project to path
project_root = Path(__file__).parent
sys.path.insert(0, str(project_root))

# Load .env if present
from dotenv import load_dotenv

load_dotenv()

# Launch the integrated v10.0 shell
from maestro_v10_integrated import main

if __name__ == "__main__":
    main()
